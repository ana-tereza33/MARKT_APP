<div id="message-<%= message.id %>" class="message message-<%= message.user == current_user ? 'right': 'left' %>" >
  <div class="fillform-chat">
    <small>
      <strong><%= message.user.name %></strong>
      <i><%= message.created_at.strftime("%a %b %e at %l:%M %p") %></i>
    </small>
    <p><%= message.content %></p>
  </div>
</div>
